<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    xmlns:card_view="http://schemas.android.com/apk/res-auto"    android:orientation="vertical"    tools:context="MainActivity"    android:background="#263238">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <android.support.design.widget.FloatingActionButton            android:id="@+id/fabscan"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentBottom="true"            android:layout_alignParentEnd="true"            android:layout_alignParentRight="true"            android:layout_gravity="bottom|end"            android:layout_marginBottom="16dp"            android:layout_marginEnd="16dp"            android:layout_marginRight="16dp"            android:src="@drawable/scan" />        <android.support.design.widget.FloatingActionButton            android:id="@+id/fabadd"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentBottom="true"            android:layout_alignParentEnd="true"            android:layout_alignParentRight="true"            android:layout_gravity="bottom|end"            android:layout_marginBottom="16dp"            android:layout_marginEnd="16dp"            android:layout_marginRight="16dp"            android:src="@drawable/add"            android:visibility="gone"/>        <ScrollView            android:layout_width="match_parent"            android:layout_height="match_parent"            android:id="@+id/scrollView">            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">                <android.support.v7.widget.CardView                    android:id="@+id/grid1"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginBottom="8dp"                    android:layout_marginStart="8dp"                    android:layout_marginEnd="8dp"                    android:layout_marginTop="8dp"                    android:background="?attr/selectableItemBackground"                    android:foreground="?attr/selectableItemBackground"                    card_view:cardCornerRadius="2dp"                    card_view:cardElevation="5dp">                    <LinearLayout                        android:id="@+id/imagelay"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical">                        <ImageView                            android:id="@+id/scannedImage"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:adjustViewBounds="true"                            android:scaleType="fitStart"                            android:src="@drawable/hui"                            />                </LinearLayout>                </android.support.v7.widget.CardView>                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_below="@id/grid1">                    <android.support.v7.widget.CardView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="8dp"                        android:layout_marginStart="8dp"                        android:layout_marginEnd="8dp"                        android:background="?attr/selectableItemBackground"                        android:foreground="?attr/selectableItemBackground"                        card_view:cardCornerRadius="2dp"                        card_view:cardElevation="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_margin="16dp"                            android:orientation="vertical"                            android:id="@+id/linearLayout">                            <LinearLayout                                android:id="@+id/container"                                android:layout_width="match_parent"                                android:orientation="vertical"                                android:layout_height="wrap_content">                            </LinearLayout>                            <View                                android:id="@+id/view1"                                android:visibility="gone"                                android:layout_marginTop="8dp"                                android:layout_width="match_parent"                                android:layout_height="1dp"                                android:background="@android:color/darker_gray"/>                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:orientation="horizontal">                                <LinearLayout                                    android:layout_marginTop="8dp"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content">                                    <ImageView                                        android:layout_width="match_parent"                                        android:layout_height="24dp"                                        android:src="@drawable/person" />                                </LinearLayout>                                <RelativeLayout                                    android:layout_marginLeft="16dp"                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content">                                    <LinearLayout                                        android:layout_width="match_parent"                                        android:orientation="vertical"                                        android:layout_height="wrap_content">                                        <EditText                                            android:id="@+id/tvname"                                            android:textSize="16sp"                                            android:hint="Name"                                            android:layout_centerVertical="true"                                            android:layout_height="wrap_content"                                            android:layout_width="match_parent"                                            />                                        <EditText                                            android:id="@+id/tvcompany"                                            android:textSize="16sp"                                            android:hint="Company"                                            android:layout_centerVertical="true"                                            android:layout_height="wrap_content"                                            android:layout_width="match_parent"                                            />                                        <EditText                                            android:id="@+id/tvtitle"                                            android:textSize="16sp"                                            android:hint="Title"                                            android:layout_centerVertical="true"                                            android:layout_height="wrap_content"                                            android:layout_width="match_parent"                                            />                                    </LinearLayout>                                </RelativeLayout>                            </LinearLayout>                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:orientation="horizontal">                                <LinearLayout                                    android:layout_marginTop="8dp"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content">                                    <ImageView                                        android:layout_width="24dp"                                        android:layout_height="24dp"                                        android:src="@drawable/call"/>                                </LinearLayout>                                <RelativeLayout                                    android:layout_marginLeft="16dp"                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content">                                    <LinearLayout                                        android:layout_width="match_parent"                                        android:layout_height="wrap_content"                                        android:orientation="vertical">                                        <EditText                                            android:id="@+id/tvmobile"                                            android:textSize="16sp"                                            android:hint="Mobile"                                            android:layout_centerVertical="true"                                            android:layout_height="wrap_content"                                            android:layout_width="match_parent"                                            />                                        <EditText                                            android:id="@+id/tvlandline"                                            android:textSize="16sp"                                            android:hint="Landline"                                            android:layout_centerVertical="true"                                            android:layout_height="wrap_content"                                            android:layout_width="match_parent"                                            />                                    </LinearLayout>                                </RelativeLayout>                            </LinearLayout>                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:orientation="horizontal">                                <LinearLayout                                    android:layout_marginTop="8dp"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content">                                    <ImageView                                        android:layout_width="24dp"                                        android:layout_height="24dp"                                        android:src="@drawable/email"/>                                </LinearLayout>                                <RelativeLayout                                    android:layout_marginLeft="16dp"                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content">                                    <EditText                                        android:id="@+id/tvemail"                                        android:textSize="16sp"                                        android:hint="Email"                                        android:layout_centerVertical="true"                                        android:layout_height="wrap_content"                                        android:layout_width="match_parent"                                        />                                </RelativeLayout>                            </LinearLayout>                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:orientation="horizontal">                                <LinearLayout                                    android:layout_marginTop="8dp"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content">                                    <ImageView                                        android:layout_width="24dp"                                        android:layout_height="24dp"                                        android:src="@drawable/website"/>                                </LinearLayout>                                <RelativeLayout                                    android:layout_marginLeft="16dp"                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content">                                    <EditText                                        android:id="@+id/tvweb"                                        android:textSize="16sp"                                        android:hint="Webpage"                                        android:layout_centerVertical="true"                                        android:layout_height="wrap_content"                                        android:layout_width="match_parent"                                        />                                </RelativeLayout>                            </LinearLayout>                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:orientation="horizontal">                                <LinearLayout                                    android:layout_marginTop="8dp"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content">                                    <ImageView                                        android:layout_width="24dp"                                        android:layout_height="24dp"                                        android:src="@drawable/home"/>                                </LinearLayout>                                <RelativeLayout                                    android:layout_marginLeft="16dp"                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content">                                    <EditText                                        android:id="@+id/tvhome"                                        android:textSize="16sp"                                        android:hint="Address"                                        android:layout_centerVertical="true"                                        android:layout_height="wrap_content"                                        android:layout_width="match_parent"                                        />                                </RelativeLayout>                            </LinearLayout>                        </LinearLayout>                    </android.support.v7.widget.CardView>                </RelativeLayout>            </RelativeLayout>        </ScrollView>    </RelativeLayout></LinearLayout>